    "stopline:no-getdb-runtime": "powershell -ExecutionPolicy Bypass -File tools/stopline-no-getdb-runtime.ps1",
    "stopline:repo-hygiene": "powershell -ExecutionPolicy Bypass -File tools/stopline-repo-hygiene.ps1",
    "docs:skeleton": "powershell -ExecutionPolicy Bypass -File tools/write-repo-skeleton.ps1",
    "gold:pull": "tsx src/tools/gold/pull-gold-memories.ts",
    "gold:push": "tsx src/tools/gold/push-gold-memories.ts",
    "gold:push-candidates": "tsx src/tools/gold/push-gold-candidates.ts",
